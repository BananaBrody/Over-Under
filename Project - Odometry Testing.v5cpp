  V5 project                                                */\n/*                                                                            */\n/*----------------------------------------------------------------------------*/\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint main() {\n  x1 = 0;\n  x2 = 0;\n  void goTo(x2,y2) {\n    width = x2 - x1;\n    height = y2 - y1;\n    Drivetrain.turnToHeading(math.atan(height / width) * 180 / math.pi), degrees);\n    Drivetrain.driveFor(forward, math.sqrt(x2 * x2 + y2 * y2), inches);\n  }\n}\n","textLanguage":"cpp","rconfig":[{"port":[2,3,4],"name":"Drivetrain","customName":false,"deviceType":"Drivetrain","deviceClass":"smartdrive","setting":{"type":"2-motor","wheelSize":"wheel4in","gear":"ratio18_1","gearRatio":"1:1","direction":"fwd","gyroType":"inertial","width":"295","unit":"mm","wheelbase":"40","wheelbaseUnit":"mm","xOffset":"0","xOffsetUnit":"mm","yOffset":"0","yOffsetUnit":"mm","thetaOffset":"180"}}],"slot":0,"platform":"V5","sdkVersion":"20220726.10.00.00","appVersion":"2.4.5","minVersion":"2.4.0","fileFormat":"1.2.0","icon":"","targetBrainGen":"First","target":"Physical"}